{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Yarn Start",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "start"
            ],
            "restart": true,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
        }
    ]
}